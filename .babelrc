{
  "env": {
    "development": {
      "presets": [["env",
        {
          "targets": {
            "browsers": [
              "last 2 Chrome versions"
            ]
          }
        }
      ]],
      "plugins": [
        "add-module-exports",
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": [[
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 Chrome versions"
            ]
          }
        }],
        "minify"
      ],
      "plugins": [
        "add-module-exports",
        "transform-object-rest-spread"
      ]
    }
  }
}
